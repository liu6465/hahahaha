#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
int n,i,t=0,max=0;
int a[]={0,0,0,0,0,0,0};
scanf("%d",&n);
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
if(t<0)
t=a[i];
else
t=t+a[i];
if(max<t)
max=t;
}
printf("×Ö¶Î×î´óºÍ:%d\n",max);
system("pause");
return 0;
}
